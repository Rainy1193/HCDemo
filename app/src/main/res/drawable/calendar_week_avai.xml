<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:left="1dp"
        android:right="1dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="-45">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="0.5dip"
                    android:color="@color/colorGrey" />
            </shape>
        </rotate>
    </item>
</layer-list>