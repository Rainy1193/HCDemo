<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">

            <corners
                android:bottomLeftRadius="100dp"
                android:bottomRightRadius="100dp"
                android:topLeftRadius="100dp"
                android:topRightRadius="100dp"/>
            <size
                android:width="100dp"
                android:height="100dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/colorBlack"/>
        </shape>
    </item>

    <item
        android:left="1dp"
        android:right="1dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="-45">
            <shape
                android:shape="line"
                android:top="1dp">
                <stroke
                    android:width="1dp"
                    android:color="@color/colorGrey1"/>
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="2dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="35%"
            android:pivotY="55%"
            android:toDegrees="-45">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="1dip"
                    android:color="@color/colorGrey1"/>
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="-5dp"
        android:left="-2dp"
        android:right="2dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="20%"
            android:pivotY="70%"
            android:toDegrees="-45">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="1dip"
                    android:color="@color/colorGrey1"/>
            </shape>
        </rotate>
    </item>

    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="15%"
            android:pivotY="100%"
            android:toDegrees="-45">

            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="1dip"
                    android:color="@color/colorGrey1"/>

            </shape>

        </rotate>
    </item>

    <item
        android:left="3dp"
        android:right="0dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="65%"
            android:pivotY="45%"
            android:toDegrees="-45">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="1dip"
                    android:color="@color/colorGrey1"/>
            </shape>
        </rotate>
    </item>

    <item
        android:left="3dp"
        android:right="2dp"
        >
        <rotate
            android:fromDegrees="-45"
            android:pivotX="80%"
            android:pivotY="30%"
            android:toDegrees="-45">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="1dip"
                    android:color="@color/colorGrey1"/>
            </shape>
        </rotate>
    </item>

    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="88%"
            android:pivotY="5%"
            android:toDegrees="-45">
            <shape
                android:shape="line"
                android:top="1dp">
                <stroke
                    android:width="1dp"
                    android:color="@color/colorGrey1"/>
            </shape>
        </rotate>
    </item>

</layer-list>