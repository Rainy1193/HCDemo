<?xml version="1.0" encoding="utf-8"?>
<com.homecaravan.android.consumer.widget.CustomNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBgFragment"
    android:scrollbarThumbVertical="@null">

    <RelativeLayout
        android:id="@+id/layoutMain"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/itemRealtor"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:layout_marginTop="@dimen/margin_item_my_team"
            android:background="@drawable/bg_save_search_item_1"
            android:elevation="1dp">

            <View
                android:id="@+id/vHeaderRealtor"
                android:layout_width="40dp"
                android:layout_height="180dp"
                android:background="@color/colorLightBlue" />

            <FrameLayout

                android:id="@+id/frmHeaderRealtor"
                android:layout_width="40dp"
                android:layout_height="180dp">

                <ImageView
                    android:id="@+id/ivSearchRealtor"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="4dp"
                    android:src="@drawable/ic_search" />

                <com.homecaravan.android.consumer.widget.VerticalTextView
                    android:id="@+id/tvHeaderRealtor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="Realtor"
                    android:textColor="@color/white"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </FrameLayout>

            <LinearLayout
                android:id="@+id/lnSelectedRealtor"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/margin_item_my_team"
                android:layout_marginLeft="@dimen/margin_item_my_team"
                android:layout_marginRight="@dimen/margin_item_my_team"
                android:layout_marginTop="@dimen/margin_item_my_team"
                android:layout_toEndOf="@+id/vHeaderRealtor"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/itemTopRealtor"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center">

                            <com.homecaravan.android.ui.CircleImageView
                                android:id="@+id/imgAvatarRealtor"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:layout_gravity="center" />

                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="12dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingTop="4dp">

                            <TextView
                                android:id="@+id/tvRealtorName"
                                style="@style/TextViewItemDashboard3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="1"
                                android:textColor="@color/colorDashboardText"
                                android:textSize="15sp" />

                            <TextView
                                android:id="@+id/tvRealtorJob"
                                style="@style/TextViewItemDashboard3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:maxLines="1"
                                android:textColor="@color/colorBottomBar"
                                android:textSize="13sp" />

                            <ImageView
                                android:id="@+id/imgRealtorLogo"
                                android:layout_width="50dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:layout_marginTop="4dp" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:orientation="horizontal"
                        android:paddingTop="4dp">

                        <LinearLayout
                            android:id="@+id/lnHistoryRealtor"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgHistoryRealtor"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_view_profile" />

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="View profile"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="4dp"
                            android:background="@color/colorLine" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgCallRealtor"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ic_change_agent" />

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="Change agent"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>


                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="4dp"
                            android:background="@color/colorLine" />

                        <LinearLayout
                            android:id="@+id/lnTeamAgentMessage"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgMessageRealtor"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ic_message_team_empty" />

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="Message"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout

                android:id="@+id/lnRealtorList"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_toEndOf="@id/vHeaderRealtor"
                android:paddingBottom="@dimen/margin_item_my_team"
                android:paddingTop="@dimen/margin_item_my_team">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/rvRealtorFeatured"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/itemLender"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:layout_below="@id/itemRealtor"
            android:layout_marginTop="@dimen/margin_item_my_team"
            android:background="@drawable/bg_save_search_item_1"
            android:elevation="1dp">

            <View
                android:id="@+id/vHeaderLender"
                android:layout_width="40dp"
                android:layout_height="180dp"
                android:background="@color/colorLightBlue" />

            <FrameLayout
                android:id="@+id/frmHeaderLender"
                android:layout_width="40dp"
                android:layout_height="180dp">

                <ImageView
                    android:id="@+id/ivSearchLender"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="4dp"
                    android:src="@drawable/ic_search" />

                <com.homecaravan.android.consumer.widget.VerticalTextView
                    android:id="@+id/tvHeaderLender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="Lender"
                    android:textColor="@color/white"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </FrameLayout>

            <LinearLayout
                android:id="@+id/lnSelectedLender"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/margin_item_my_team"
                android:layout_marginLeft="@dimen/margin_item_my_team"
                android:layout_marginRight="@dimen/margin_item_my_team"
                android:layout_marginTop="@dimen/margin_item_my_team"
                android:layout_toRightOf="@id/vHeaderLender"
                android:orientation="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/itemTopLender"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center">

                            <com.homecaravan.android.ui.CircleImageView
                                android:id="@+id/imgAvatarLender"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:layout_gravity="center" />

                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="12dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingTop="4dp">

                            <TextView
                                android:id="@+id/tvLenderName"
                                style="@style/TextViewItemDashboard3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="2"
                                android:textColor="@color/colorDashboardText" />

                            <TextView
                                android:id="@+id/tvLenderJob"
                                style="@style/TextViewItemDashboard3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:maxLines="1"
                                android:textColor="@color/colorBottomBar"
                                android:textSize="13sp" />

                            <ImageView
                                android:id="@+id/imgLenderLogo"
                                android:layout_width="50dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start" />
                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:orientation="horizontal"
                        android:paddingTop="4dp">

                        <LinearLayout
                            android:id="@+id/lnHistoryLender"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgHistoryLender"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ic_view_profile" />


                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="View profile"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="4dp"
                            android:background="@color/colorLine" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgCallLender"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ic_change_agent" />

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="Change agent"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="4dp"
                            android:background="@color/colorLine" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgMessageLender"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ic_message_team" />

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="New message"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lnLenderList"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_toRightOf="@id/vHeaderLender"
                android:paddingBottom="@dimen/margin_item_my_team"
                android:paddingTop="@dimen/margin_item_my_team"
                android:visibility="visible">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/rvLenderFeatured"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/itemFriendFamily"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:layout_below="@id/itemLender"
            android:layout_marginTop="8dp"
            android:background="@drawable/bg_save_search_item_1"
            android:elevation="1dp">

            <View
                android:id="@+id/vHeaderFriendFamily"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:background="@color/colorLightBlue" />

            <RelativeLayout
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSearchFriend"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="4dp"
                        android:src="@drawable/ic_search" />

                    <com.homecaravan.android.consumer.widget.VerticalTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="8dp"
                        android:rotation="180"
                        android:text="Friend / Family"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginBottom="@dimen/margin_item_my_team"
                android:layout_marginTop="@dimen/margin_item_my_team"
                android:layout_toEndOf="@id/vHeaderFriendFamily"
                android:orientation="horizontal">

                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scrollbarThumbHorizontal="@null">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:id="@+id/layoutFriend"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginEnd="16dp"
                            android:layout_marginStart="16dp">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:orientation="vertical">

                                <FrameLayout
                                    android:layout_width="80dp"
                                    android:layout_height="80dp"
                                    android:background="@drawable/bg_image_my_team_consumer">

                                    <ImageView
                                        android:id="@+id/imgFamilyAdd"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        android:src="@drawable/ic_add_collaborator" />
                                </FrameLayout>

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:text="Add"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>

                        </RelativeLayout>

                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/rvFriendFamily"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginStart="8dp" />

                    </LinearLayout>
                </HorizontalScrollView>

            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/itemHomeInspector"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:layout_below="@id/itemFriendFamily"
            android:layout_marginTop="@dimen/margin_item_my_team"
            android:background="@drawable/bg_save_search_item_1"
            android:elevation="1dp">

            <View
                android:id="@+id/vHeaderHomeInspector"
                android:layout_width="40dp"
                android:layout_height="180dp"
                android:background="@color/colorLightBlue" />

            <FrameLayout
                android:id="@+id/frmHeaderHomeInspector"
                android:layout_width="40dp"
                android:layout_height="180dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSearchHomeInspector"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="4dp"
                        android:src="@drawable/ic_search" />

                    <com.homecaravan.android.consumer.widget.VerticalTextView
                        android:id="@+id/tvHeaderHomeInspector"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="8dp"
                        android:rotation="180"
                        android:text="Home Inspector"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>

            </FrameLayout>

            <LinearLayout
                android:id="@+id/lnSelectedHomeInspector"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/margin_item_my_team"
                android:layout_marginLeft="@dimen/margin_item_my_team"
                android:layout_marginRight="@dimen/margin_item_my_team"
                android:layout_marginTop="@dimen/margin_item_my_team"
                android:layout_toRightOf="@id/vHeaderHomeInspector"
                android:orientation="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/itemTopHomeInspector"
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center">

                            <com.homecaravan.android.ui.CircleImageView
                                android:id="@+id/imgAvatarHomeInspector"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:layout_gravity="center" />

                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="12dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingTop="4dp">

                            <TextView
                                android:id="@+id/tvHomeInspectorName"
                                style="@style/TextViewItemDashboard3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorDashboardText" />

                            <TextView
                                android:id="@+id/tvHomeInspectorJob"
                                style="@style/TextViewItemDashboard3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:maxLines="1"
                                android:textColor="@color/colorBottomBar"
                                android:textSize="13sp"
                                android:textStyle="bold" />

                            <ImageView
                                android:id="@+id/imgHomeInspectorLogo"
                                android:layout_width="50dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:orientation="horizontal"
                        android:paddingTop="4dp">

                        <LinearLayout
                            android:id="@+id/lnHistoryHomeInspector"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgHistoryHomeInspector"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ic_consumer_myteam_history" />

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="History"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="4dp"
                            android:background="@color/colorLine" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgCallHomeInspector"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ic_consumer_myteam_call_color" />

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="Call"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="4dp"
                            android:background="@color/colorLine" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imgMessageHomeInspector"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/ic_consumer_myteam_message" />

                                <TextView
                                    style="@style/TextViewItemDashboard3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="8dp"
                                    android:gravity="center"
                                    android:text="Message"
                                    android:textColor="@color/colorDashboardText" />
                            </LinearLayout>


                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lnHomeInspectorList"
                android:layout_width="match_parent"
                android:layout_height="208dp"
                android:layout_toRightOf="@id/vHeaderHomeInspector"
                android:paddingBottom="@dimen/margin_item_my_team"
                android:paddingTop="@dimen/margin_item_my_team"
                android:visibility="visible">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/rvHomeInspectorFeatured"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/itemTitleInsurance"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:layout_below="@id/itemHomeInspector"
            android:layout_marginTop="8dp"
            android:background="@drawable/bg_save_search_item_1"
            android:elevation="1dp">

            <View
                android:id="@+id/vTitleInsurance"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:background="@color/colorLightBlue" />

            <RelativeLayout
                android:layout_width="40dp"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSearchTitleInsurance"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="4dp"
                        android:src="@drawable/ic_search" />

                    <com.homecaravan.android.consumer.widget.VerticalTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="8dp"
                        android:gravity="center_horizontal"
                        android:rotation="180"
                        android:text="Title Insurance"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>

            </RelativeLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginBottom="@dimen/margin_item_my_team"
                android:layout_marginTop="@dimen/margin_item_my_team"
                android:layout_toEndOf="@id/vTitleInsurance"
                android:background="@color/white">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/rvTitleInsurance"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical" />

            </FrameLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/itemEscrowCompany"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:layout_below="@id/itemTitleInsurance"
            android:layout_marginTop="8dp"
            android:background="@drawable/bg_save_search_item_1"
            android:elevation="1dp">

            <View
                android:id="@+id/vEscrowCompany"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:background="@color/colorLightBlue" />

            <RelativeLayout
                android:layout_width="40dp"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSearchEscrowCompany"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="4dp"
                        android:src="@drawable/ic_search" />

                    <com.homecaravan.android.consumer.widget.VerticalTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:gravity="center"
                        android:rotation="180"
                        android:text="Escrow Company"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>

            </RelativeLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginBottom="@dimen/margin_item_my_team"
                android:layout_marginTop="@dimen/margin_item_my_team"
                android:layout_toEndOf="@id/vEscrowCompany"
                android:background="@color/white">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/rvEscrowCompany"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical" />

            </FrameLayout>

        </RelativeLayout>

    </RelativeLayout>

